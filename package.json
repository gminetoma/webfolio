{
  "name": "husky",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "build-preview": "yarn build; yarn preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
